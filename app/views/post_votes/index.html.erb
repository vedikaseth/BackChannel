<h1>Listing post_votes</h1>

<table>
  <tr>
    <th>user</th>
    <th>post</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @post_votes.each do |post_vote| %>
  <tr>

    <td>  <%=post_vote.user_id %>    </td>
    <td>  <%=post_vote.post_id %>    </td>


    <td><%= link_to 'Show', post_vote %></td>
    <td><%= link_to 'Edit', edit_post_vote_path(post_vote) %></td>
    <td><%= link_to 'Destroy', post_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post vote', new_post_vote_path %>
